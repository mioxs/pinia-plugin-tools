{
  "name": "pinia-plugin-store",
  "version": "2.2.2",
  "description": "pinia tools plugin",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    },
    "./*": "./*"
  },
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "scripts": {
    "test": "vitest",
    "eslint": "eslint --fix **/*.{ts,js}",
    "rollup": "rollup -c rollup.config.js --bundleConfigAsCjs",
    "build": "npm run rollup && npm run eslint"
  },
  "dependencies": {
    "pinia": "^2.1.7",
    "vue": "^3.4.33"
  },
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.24.8",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.14.11",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/test-utils": "^2.4.6",
    "crypto-js": "^4.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "happy-dom": "^14.12.3",
    "prettier": "^3.3.3",
    "rollup": "^4.19.0",
    "rollup-plugin-dts": "^6.1.1",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3",
    "vite": "^5.3.4",
    "vitest": "^2.0.3"
  },
  "author": "mioxs",
  "license": "MIT",
  "keywords": [
    "vue",
    "pinia",
    "persist",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mioxs/pinia-plugin-store.git"
  },
  "bugs": {
    "url": "https://github.com/mioxs/pinia-plugin-store/issues"
  },
  "homepage": "https://github.com/mioxs/pinia-plugin-store#readme"
}